<?xml version="1.0" encoding="utf-8"?>
<project>
	<icon path="Assets/favicons/normal.png" />
	
	<meta title="Intellector" package="com.sample.intellector" version="1.0.0" company="Company Name" />
	<app main="Main" path="Export" file="1646787907" preloader="Preloader" />
	<window background="#EEEEEE" />
	
	<source path="Source" />
	
	<haxelib name="openfl" />
	<haxelib name="svg" />
	<haxelib name="hxWebSockets" />
	<haxelib name="haxeui-core" />
	<haxelib name="haxeui-openfl" />
	<haxelib name="polygonal-ds" />
	<haxelib name="FileSaver" />
	<haxelib name="gif" />
	<haxelib name="format" />
	<haxelib name="haxe-strings" />
	
	<template path="template.html" rename="index.html" />
	<library path="Assets/preloader.swf" generate="true"  id="preloader" preload="true" embed="false" />

	<section if="unit">
		<haxelib name="utest" />
		<template path="templates" />
	</section>

	<haxedef name="haxeui_nofilters" />
	<haxeflag name="--resource" value="./Source/tests/ui/test_case_infos.json@test_case_infos" if="localtests" />
	<haxeflag name="--resource" value="./Assets/resources/changelog.json@changelog" />
	
	<assets path="Assets/sounds" rename="sounds" />
	<assets path="Assets" rename="assets" />
	<assets path="Assets/fonts" rename="fonts" />
	<assets path="Assets/layouts" rename="layouts" />
	
</project>
