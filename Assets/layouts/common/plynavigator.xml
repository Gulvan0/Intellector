<vbox width="100%" height="100%" style="border-top: 0; border-left: 1; border-right: 1; border-bottom: 1;border-color: #dddddd;padding: 1 1 1 1;spacing:0px">
    <button-bar id="scrollBtnBar" toggle="false" width="100%">
        <button id="homeBtn" text="assets/symbols/common/home.svg" width="25%" />
        <button id="prevBtn" icon="assets/symbols/common/prev.svg" width="25%" />
        <button id="nextBtn" icon="assets/symbols/common/next.svg" width="25%" />
        <button id="endBtn" icon="assets/symbols/common/end.svg" width="25%"  />
    </button-bar>
    <tableview id="movetable" width="100%" height="100%" contentWidth="100%">
        <header width="100%" height="0px">
            <column width="13%" id="num" />
            <column width="43.5%" id="white_move" />
            <column width="43.5%" id="black_move" />
        </header>
    </tableview>  
</vbox>